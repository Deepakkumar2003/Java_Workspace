class Numberpt
{
	public static void main(String[] args)
	{
	int n=5;
    for(int i=1; i<=n; i++) {
	
	for(int j=i; j<=n-1; j++) {
		   System.out.print("\t");
	}
	if(i%2!=0) {
		int k=i*i-2*i+2;
		for(int j=1;j<=2*i-1;j++) {
			System.out.print("\t" +k++ );
		}
	}
	else {
		int k=i*i;
		for(int j=1;j<=2*i-1;j++) {
			System.out.print( "\t"+ k--  );
		}
	}
	       System.out.println();
}
}
}